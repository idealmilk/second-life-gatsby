{
  "name": "sales-site-gatsby",
  "version": "1.1.13",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --fix src/**/*.js",
    "prettier": "prettier --write src/**/*.js",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@reach/router": "^1.3.4",
    "babel-plugin-styled-components": "^1.13.3",
    "framer-motion": "^4.1.17",
    "gatsby": "^4.18.2",
    "gatsby-core-utils": "^3.11.1",
    "gatsby-plugin-canonical-urls": "^4.12.1",
    "gatsby-plugin-image": "^2.11.1",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-preact": "^6.12.1",
    "gatsby-plugin-react-helmet": "^5.12.1",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^4.11.1",
    "gatsby-plugin-sitemap": "^5.12.1",
    "gatsby-plugin-styled-components": "^5.12.1",
    "gatsby-source-contentful": "^7.22.0",
    "gatsby-transformer-sharp": "^4.12.1",
    "postcss": "^8.4.12",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "puppeteer": "^13.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^1.14.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "gatsby-plugin-root-import": "^2.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^11.2.6",
    "prettier": "2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix"
    ],
    "*.{js,htm,html,css,scss,less,graphql,json,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
